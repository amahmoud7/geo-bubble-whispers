# Environment Configuration
NODE_ENV=production

# Supabase Configuration
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key

# Google Maps API
# You can provide platform-specific keys if needed. At least one key is required.
# Get your API key from: https://console.cloud.google.com/google/maps-apis/credentials
# Required permissions: Maps JavaScript API, Places API
VITE_GOOGLE_MAPS_API_KEY=your-google-maps-api-key
# Optional native overrides (e.g., bundle-restricted keys)
VITE_GOOGLE_MAPS_API_KEY_IOS=
VITE_GOOGLE_MAPS_API_KEY_ANDROID=
VITE_VAPID_PUBLIC_KEY=your-vapid-public-key

# App Configuration
VITE_APP_NAME="Geo Bubble Whispers"
VITE_APP_VERSION=1.0.0
VITE_API_BASE_URL=https://your-api.com

# Analytics (Optional)
VITE_ANALYTICS_ID=your-analytics-id

# Sentry (Optional)
VITE_SENTRY_DSN=your-sentry-dsn

# Feature Flags
VITE_ENABLE_DEV_TOOLS=false
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_CRASH_REPORTING=true